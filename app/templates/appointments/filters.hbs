<div class="btn-toolbar" role="toolbar">
  <div class="btn-group">
    {{#each visit_types}}
      <button
        {{action 'filterByVisitType' this.id}}
        type="button"
        {{bind-attr class=":btn :btn-default this.selected:btn-info"}}>
          {{this.name}}
      </button>
    {{/each}}
  </div>
  <div class="btn-group">
    {{#each priorities}}
      <button
        {{action 'filterByPriority' this.id}}
        type="button"
        {{bind-attr class=":btn :btn-default this.selected:btn-info"}}>
          {{this.name}}
      </button>
    {{/each}}
  </div>
  <div class="btn-group">
    {{#each service_options}}
      <button
        {{action 'filterByServiceOption' this.id}}
        type="button"
        class="btn btn-default
        {{bind-attr class=":btn :btn-default this.selected:btn-info"}}>
          {{this.name}}
      </button>
    {{/each}}
  </div>
</div>
<br/>
<div class="btn-toolbar" role="toolbar">
  <div class="btn-group">
    {{#each dates}}
      <button
        {{action 'filterByDate' this.date}}
        type="button"
        {{bind-attr class=":btn :btn-default this.selected:btn-info"}}>
          {{this.date}}
      </button>
    {{/each}}
  </div>
</div>
