{{#if navbar}}
  <form class="navbar-form navbar-left" role="search">
    <div class="form-group">
      {{input type="text" value=searchString placeholder=placeholderString class="form-control"}}
    </div>
  </form>
{{else}}
  {{input type="text" value=searchString placeholder=placeholderString}}
{{/if}}

<ul class="search-results">
  {{#each searchResults}}
    <li {{action 'resultSelected' this}}>{{personal_fields.name}} - {{hospital_number}}</li>
  {{/each}}
</ul>
